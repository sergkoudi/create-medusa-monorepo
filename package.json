  "author": "Sergei Kudinov",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^11.0.0",
    "inquirer": "^12.6.1",
    "slugify": "^1.6.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.4",
    "@types/commander": "^2.12.5",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.15.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  },
  "packageManager": "yarn@4.5.2"
