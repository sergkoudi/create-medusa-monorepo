{
  "name": "create-medusa-monorepo",
  "version": "0.1.2",
  "description": "CLI tool to scaffold Medusa monorepo with plugins and examples",
  "exports": "./dist/index.js",
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sergkoudi/create-medusa-monorepo.git"
  },
  "scripts": {
